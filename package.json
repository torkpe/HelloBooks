{
  "name": "hellobooks",
  "version": "1.0.0",
  "description": "an e-library system",
  "main": "server.js",
  "scripts": {
    "build": "babel ./server/src --watch -d ./server/dist --presets es2015",
    "start:dev": "nodemon ./server/dist/bin/www.js --watch",
    "runtest": "babel ./server/test --watch -d ./server/runtest --presets es2015",
    "test": "cross-env-shell NODE_ENV=travis && sequelize db:migrate:undo:all && sequelize db:migrate && nyc mocha ./server/runtest/*",
    "test:local": "cross-env-shell NODE_ENV=test && sequelize db:migrate:undo:all && sequelize db:migrate && nyc mocha ./server/runtest/*",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ./server/src/server.js",
    "start": "node ./server/dist/bin/www.js && db:migrate",
    "test:db": "sequelize db:migrate",
    "heroku-postbuild": "NODE_ENV=production  && NODE_END=production sequelize db:migrate:undo:all && NODE_END=production sequelize db:migrate && npm run db:seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torkpe/HelloBooks.git"
  },
  "keywords": [
    "hellobooks"
  ],
  "author": "Temitope Emmanuel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/torkpe/HelloBooks/issues"
  },
  "homepage": "https://github.com/torkpe/HelloBooks#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "chai": "^4.1.0",
    "cookie-parser": "^1.4.3",
    "coveralls": "^2.13.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-mailer": "^0.3.1",
    "express-session": "^1.15.5",
    "faker": "^4.1.0",
    "jsonwebtoken": "^7.4.1",
    "md5": "^2.2.1",
    "mocha": "^3.5.0",
    "morgan": "^1.8.2",
    "nodemailer": "^4.0.1",
    "nyc": "^11.1.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "pg": "^7.0.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.4.2",
    "sequelize-cli": "^2.8.0",
    "supertest": "^3.0.0"
  }
}
