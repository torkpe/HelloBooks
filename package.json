{
  "name": "hellobooks",
  "version": "1.0.0",
  "description": "an e-library system",
  "main": "server.js",
  "scripts": {
    "e2e-setup": "selenium-standalone install",
    "e2e-test": " NODE_ENV=test nightwatch",
    "build": "webpack --config ./webpack.config.production.js",
    "start": "babel-node ./server/src/bin/www",
    "test": "cross-env-shell NODE_ENV=travis && sequelize db:migrate:undo:all && sequelize db:migrate && nyc mocha --compilers js:babel-core/register ./server/src/test/* --timeout 10000ms && npm run jest",
    "seed": "sequelize db:migrate:undo:all && sequelize db:migrate && babel-node ./server/src/seed",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:local": "cross-env-shell NODE_ENV=test && sequelize db:migrate:undo:all && sequelize db:migrate && nyc --reporter=lcov --reporter=text mocha --compilers js:babel-core/register ./server/src/test/* --timeout 5000ms",
    "heroku-postbuild": "NODE_ENV=production  && NODE_END=production && npm run build",
    "jest": "jest --u ./client/test/* --coverage"
  },
  "engines": {
    "node": "v8.9.x",
    "npm": "5.5.x"
  },
  "jest": {
    "coverageDirectory": "./jest-coverage/",
    "collectCoverage": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "./client/styles/*",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.worker.js": "identity-obj-proxy"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "setupFiles": [
      "jest-localstorage-mock",
      "./client/test/__mock__/browserHistory.js"
    ]
  },
  "dependencies": {
